MyData <- as.matrix(read.table('head_2000'))
af <- rowMeans(MyData)
het <- af*(1 - af)
Fst <- as.matrix(read.table('Fst2000'))
plot(Fst, het)
plot(het, Fst)
plot(het, Fst, pch = 19, cex = .4)
plot(het, Fst, pch = 19, cex = .4, xlab = "Hétérozigotie")
plot(af, Fst, pch = 19, cex = .4, xlab = "Hétérozigotie")
plot(het, Fst, pch = 19, cex = .4, xlab = "Hétérozigotie")
pdf('Fst_het.pdf', useDingbats=FALSE)
plot(het, Fst, pch = 19, cex = .4, xlab = "Hétérozigotie")
dev.off()
